      <xs:element name="RsltnRltdInf" type="ResolutionInformation2" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN" source="Name">Resolution Related Information</xs:documentation>
          <xs:documentation xml:lang="EN" source="Definition">Reference of a return or a reversal initiated to fix the case under investigation as part of the resolution.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
